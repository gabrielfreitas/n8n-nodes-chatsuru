{
  "name": "n8n-nodes-chatsuru",
  "version": "1.3.0",
  "description": "n8n node para integração com Chatsuru API",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "copy:assets": "gulp copyAssets",
    "build": "npm run clean && tsc && npm run copy:assets",
    "prepublish": "npm run build"
  },
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "chatsuru",
    "tsuru",
    "blubots"
  ],
  "author": "Jean Jr Dev",
  "license": "MIT",
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/ChatsuruApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Chatsuru/Chatsuru.node.js",
      "dist/nodes/Chatsuru/ChatsuruProduct.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "del": "^8.0.1",
    "gulp": "^5.0.1",
    "gulp-typescript": "^6.0.0-alpha.1",
    "n8n-workflow": "^1.113.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.4.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
